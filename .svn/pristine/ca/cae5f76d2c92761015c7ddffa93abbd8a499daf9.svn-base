/* Reset */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td{margin:0;padding:0;}table{border-collapse:collapse;border-spacing:0;margin:0;padding:0;}fieldset,img{display:block; border:0;}address,caption,cite,code,dfn,em,strong,th,var{font-style:normal;font-weight:normal;}ol,ul {list-style:none;}caption,th {text-align:left;}h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}q:before,q:after{content:'';}abbr,acronym {border:0;}

body {
	font-family: 'HelveticaNeueLTStd-Md';
	font-size:16px;
}
b, strong {
	font-family: 'HelveticaNeueLTStd-Bd';
}
em {
	font-style:italic;
}
a {
	text-decoration:none;
}
* {
	-webkit-box-sizing:border-box;
	-moz-box-sizing:border-box;
	box-sizing:border-box;
}
.wrapper {
	width:100%;
	min-height:100vh;
	position:relative;
	display:block;
	background:url(../images/BG_purplegradient.png) repeat-x center bottom;
	background-attachment:fixed;
	overflow:hidden;
}
.wrapper.about {
	height:1750px;
	background:url(../images/BG_elephant.png) no-repeat top center;
	background-size:cover;
}
.headerWrapper {
	width:100%;
	max-width:1080px;
	margin:0 auto;
	position:relative;
}
header {
	text-align:center;
	padding:35px 0;
	float:left;
}
header .logo {
	width:177px;
	height:33px;
	background:url(../images/logo_pediasure.png) no-repeat center center;
	display:block;
	margin:0 auto;
}
nav {
	float:right;
	position:relative;
	z-index:1;
}
nav ul {
	text-align:center;
	padding:40px 0;
}
nav ul li {
	display:inline-block;
	margin:0 15px;
}
nav ul li a {
	font-family: 'GillSansMT';
	font-weight:600;
	color:#6c3382;
}
nav ul li a.active,
nav ul li a:hover {
	text-decoration:underline;
}
.mobilecloseBtn {
	width:28px;
	height:24px;
	background:url(../images/icon_close.png) no-repeat center center;
	display:none;
	text-indent:-9999px;
	position:absolute;
	top:30px;
	right:30px;
}
.burgerIcon {
	width:30px;
	height:25px;
	background:url(../images/icon_burger.png) no-repeat center center;
	text-indent:-9999px;
	display:none;
	position:absolute;
	top:30px;
	right:20px;
}
.content {
	width:100%;
	max-width:1080px;
	margin:0 auto;
	padding-bottom:100px;
}
.content h3.tag {
	width:100%;
	font-family: 'abbottsangregular';
	font-size:40px;
	color:#6d3483;
	text-align:center;
	display:inline-block;
	margin-bottom:35px;
	position:relative;
}
.content h3.tag span {
	font-size:24px;
	position:absolute;
	bottom:-24px;
	left:68%;
	white-space:nowrap;
}
.content h2 {
	font-size:24px;
	color:#6c3382;
}
.content p {
	color:#6c3382;
	line-height:1.4;
}
.contentImg {
	width:80%;
	max-width:414px;
	float:left;
}
.contentImg img {
	width:100%;
}
.content ul {
	margin-left:15px;
	float:left;
	margin-right:100px;
}
.content ul li {
	list-style:disc;
	padding-left:20px;
	line-height:1.5;
	color:#6c3382;
}
.about .content {
	max-width:830px;
	margin:0 0 0 60px;
}
.about .content h4 {
	font-family: 'HelveticaNeueLTStd-Bd';
	font-size:25px;
	color:#6c3382;
}
.about .content ul li {
	padding-left:0;
}
.productDesc {
	display:block;
	float:left;
	margin-top:20px;
	margin-bottom:50px;
}
.productDesc .image {
	width:25%;
	max-width:127px;
	float:left;
	margin-right:20px;
}
.productDesc .image img {
	width:100%;
}
.productDesc .caption {
	float:left;
	padding-top:30px;
}
.motto {
	width:100%;
	max-width:533px;
}
.motto img {
	width:100%;
	margin:0 auto;
}
footer {
	width:100%;
	position:absolute;
	bottom:0;
	left:0;
	display:block;
	overflow:hidden;
}
.footerWrapper {
	width:100%;
	height:300px;
	position:relative;
}
.footerWrapper .footerRibbon {
	width:100%;
	min-width:640px;
	display:block;
	position:absolute;
	bottom:0;
	right:0;
}
.footerWrapper .footercanCaption {
	min-width:70px;
	max-width:162px;
	width:11%;
	max-width:162px;
	display:block;
	position:absolute;
	bottom:0;
	right:6%;
}
.defBtn {
	font-family: 'blissboldregular';
	font-size:18px;
	color:#fff;
	background:#6d3483;
	padding:15px 30px;
	border-radius:25px;
	display:inline-block;
	text-align:center;
	margin-bottom:10px;
	box-shadow:inset 6px 6px 10px 0 #aa88b7;
}
.defBtn.fb {
	width:100%;
	background:#3b5997;
	box-shadow:inset 4px 6px 5px 0 #8c9dc2;
	padding:10px;
}
.defBtn.twit {
	width:100%;
	background:#2eaae2;
	box-shadow:inset 4px 6px 5px 0 #82ccee;
	margin-bottom:0;
	padding:10px;
}
.defBtn.hover {
	box-shadow:inset 0 -6px 3px #aa88b7;
}
.featureWrapper {
	width:740px;
	margin:0 auto 30px;
	display:block;
}
.featurePanel {
	width:340px;
	float:left;
}
.featurePanel:first-child {
	margin-right:60px;
}
.featurePanel .image {
	width:100%;
	margin-bottom:20px;
}
.featurePanel .image img {
	width:100%;
}
.btnContainer {
	width:100%;
	position:relative;
	display:block;
	text-align:center;
	margin:0 auto 20px;
}
.featurePanel .btnContainer .defBtn {
	width:100%;
	margin:0 auto;
}
.btnContainer .clickArea {
	width:100%;
	height:100%;
	position:absolute;
	z-index:100;
}
.featurePanel p {
	width:100%;
	font-size:13px;
	color:#6c3382;
	text-align:center;
	display:inline-block;
	vertical-align:sub;
}
.popupWrapper {
	width:100%;
	height:100%;
	position:fixed;
	top:0;
	left:0;
	z-index:99999;
	display:none;
	overflow:auto;
}
.overlay {
	width:100%;
	height:100%;
	position:fixed;
	background:#000;
	opacity:0.8;
}
.popupBox {
	width:auto;
}
.popup {
	background:#fff;
	display:none;
	position:absolute;
	top:50%;
	left:50%;
	transform:translateX(-50%)translateY(-50%);
	-webkit-transform:translateX(-50%)translateY(-50%);
	-ms-transform:translateX(-50%)translateY(-50%);
}
#signupPopup,
#sharePopup {
	width:300px;
	border:5px solid #6d3483;
	border-radius:40px;
	padding:30px 20px;
	margin:0 auto;
}
#signupPopup2 {
	width:485px;
	border:5px solid #6d3483;
	border-radius:40px;
	padding:30px 20px;
	margin:0 auto;
}
.popup .closeBtn {
	font-size:24px;
	color:#fff;
	position:absolute;
	top:-15px;
	right:-15px;
}
.popup h3 {
	width:100%;
	font-family: 'abbottsangregular';
	font-size:21px;
	color:#6d3483;
	text-align:center;
	display:inline-block;
	margin-bottom:20px;
}
.joinForm {
	padding:0 20px;
}
.joinForm table,
.childForm table {
	width:100%;
	margin-bottom:20px;
}
.joinForm td,
.childForm td {
	color:#6c3382;
	padding-bottom:15px;
}
.joinForm td:first-child,
.childForm td:first-child {
	width:35%;
}
.joinForm td:last-child,
.childForm td:last-child {
	width:65%;
}
.joinForm input {
	width:100%;
	background:#c5abd1;
	padding:15px;
	border:none;
	border-radius:20px;
	box-shadow:inset 2px 3px 10px 0 #666;
}
.joinForm input[type="radio"] {
	display:none;
}
.joinForm input[type="radio"] + label {
	font-size:16px;
    color:#6d3483;
	display:block;
	margin-bottom:20px;
}
.joinForm input[type="radio"] + label span {
    display:inline-block;
    width:20px;
    height:20px;
    margin:-1px 10px 0 0;
    vertical-align:middle;
    cursor:pointer;
    -moz-border-radius:  50%;
    border-radius:  50%;
	border:2px solid #6c3382;
	background:#fff;
}
.joinForm input[type="radio"]:checked + label span {
	background:url(../images/icon_radiochecked.png) no-repeat center center;
}
.termsCondition {
	text-align:center;
}
.disclaimer {
	background:url(../images/icon_righttriangle.png) no-repeat center right;
	display:inline-block;
	padding-right:15px;
	text-decoration:underline;
	margin-bottom:30px;
}
.periode {
	text-align:center;
	margin:0 auto 50px;
}
.uploadBtn {
	width:340px;
	height:340px;
	position:relative;
}
.dropzone {
	padding:0;
	border:none;
	box-shadow:0 0 10px -5px #333;
	border-radius:20px;
}
.dropzoneBox {
	width:100%;
	height:100%;
}
#uploadFoto {
	width:100%;
	height:100%;
}
.dropzone.dz-clickable .dz-message, .dropzone.dz-clickable .dz-message * {
	background:url(../images/img_uploadbutton.png) no-repeat center center;
	width:100%;
	height:100%;
}
.dropzone .dz-message {
	width:100%;
	margin:0;
}
.dropzone .dz-preview {
	width:100%;
	height:100%;
    margin: 0 !important;
	top:0;
	left:0;
}
.dropzone .dz-preview .dz-image {
    border-radius:0 !important;
    height: 90% !important;
    width: 100% !important;
}
.dropzone .dz-preview .dz-image img {
    height: auto;
    margin: 0 auto;
    width: 100%;
}
.uploadData {
	width:340px;
	height:340px;
	background:#fff;
	box-shadow:0 0 10px -5px #333;
	border-radius:20px;
	position:relative;
}
.uploadData .input {
	width:85%;
	position:absolute;
	top:50%;
	left:50%;
	transform:translateX(-50%)translateY(-50%);
	-web-transform:translateX(-50%)translateY(-50%);
	-ms-transform:translateX(-50%)translateY(-50%);
}
.uploadData .input p,
.uploadData .input textarea {
	font-size:18px;
	color:#6c3381;
	text-align:left;
}
.uploadData .input textarea {
	width:100%;
	height:70px;
	line-height:35px;
	padding:0 10px;
	border:none;
	background:url(../images/img_underlinedot.png) no-repeat center bottom;
	background-size:contain;
	resize:none;
	margin-bottom:10px;
}
.previewBox {
	width:236px;
	background:#fff;
	border-radius:20px;
	margin:0 auto;
	overflow:hidden;
}
.previewBox .image {
	width:236px;
	height:224px;
	position:relative;
	margin:0;
	overflow:hidden;
}
.previewBox .image img {
	width:100%;
	position:absolute;
	top:50%;
	left:50%;
	transform:translateX(-50%)translateY(-50%);
	-webkit-transform:translateX(-50%)translateY(-50%);
	-ms-transform:translateX(-50%)translateY(-50%);
}
.previewBox .captionWrapper {
	padding:10px;
}
.previewBox .caption {
	border-bottom:1px solid #666;
	padding-bottom:5px;
}
.previewBox .caption p {
	font-size:12px;
	color:#000;
	text-align:left;
}
.previewBox .captionWrapper .profile {
	padding:10px 0;
	overflow:hidden;
}
.previewBox .captionWrapper .profile .profileImage {
	width:50px;
	height:50px;
	background:#333;
	border-radius:50%;
	margin-right:10px;
	float:left;
	overflow:hidden;
}
.previewBox .captionWrapper .profile .profileImage img {
	width:100%;
	height:100%;
}
.previewBox .captionWrapper .profile p {
	width:auto;
	font-size:12px;
	color:#000;
	margin-top:18px;
	float:left;
}
.twoButton {
	display:inline-block;
}
.twoButton:first-child {
	margin-right:30px;
}
.thanksWrapper {
	width:100%;
	text-align:center;
	/*position:absolute;
	top:50%;
	left:50%;
	transform:translateX(-50%)translateY(-50%);
	-webkit-transform:translateX(-50%)translateY(-50%);
	-ms-transform:translateX(-50%)translateY(-50%);*/
	margin-top:220px;
}
.thanksWrapper p {
	font-size:18px;
	margin-bottom:40px;
	padding:0 15%;
}
.childForm {
	width:100%;
	max-width:480px;
	margin:0 auto;
}
.childForm input {
	width:100%;
	background:#fff;
	font-size:16px;
	color:#6c3382;
	padding:8px 15px;
	border:3px solid #6c3382;
	border-radius:20px;
}
.resultWrapper {
	width:60%;
	width:665px;
	margin:0 auto;
}
.personalResult {
	width:665px;
	height:202px;
	position:relative;
	margin-bottom:50px;
}
.personalResult.male .umur {
	width:128px;
	height:187px;
	background:url(../images/icon_male.png) no-repeat center center;
	display:block;
	margin:0 auto;
	position:relative;
}
.personalResult.female .umur {
	width:190px;
	height:187px;
	background:url(../images/icon_female.png) no-repeat center center;
	display:block;
	margin:0 auto;
	position:relative;
}
.personalResult .umur {
	background-size:contain !important;
}
.personalResult .umur span {
	font-size:40px;
	color:#fff;
	position:absolute;
	left:50%;
	transform:translateX(-50%);
	text-align:center;
}
.personalResult.male .umur span {
	top:75px;
}
.personalResult.female .umur span {
	top:65px;
}
.personalResult .umur p {
	width:100%;
	color:#1989c5;
	position:absolute;
	left:50%;
	bottom:-18px;
	transform:translateX(-50%);
	-webkit-transform:translateX(-50%);
	-ms-transform:translateX(-50%);
	text-align:center;
}
.beratBadan {
	width:235px;
	height:187px;
	position:absolute;
	bottom:0;
	left:0;
}
.tinggiBadan {
	width:235px;
	height:187px;
	position:absolute;
	bottom:0;
	right:0;
}
.dataWrapper {
	width:100%;
	height:100%;
	position:relative;
}
.measurement {
	width:40px;
	height:40px;
	color:#fff;
	border-radius:50%;
	position:absolute;
	top:10px;
	left:50%;
	transform:translateX(-50%);
	-webkit-transform:translateX(-50%);
	-ms-transform:translateX(-50%);
	overflow:hidden;
	z-index:10;
}
.measurement span {
	background:url(../images/img_measurepoint.png) no-repeat center center;
	width:100%;
	height:100%;
	padding:10px 0;
	display:block;
	text-align:center;
}
.circle {
	width:180px;
	height:180px;
	border-radius:50%;
	position:absolute;
	top:0;
	left:50%;
	transform:translateX(-50%);
	-webkit-transform:translateX(-50%);
	-ms-transform:translateX(-50%);
	display:block;
}
.circle.red {
	background:#e00303;
	box-shadow:inset 5px 5px 10px 10px #d76d6d;
}
.circle.orange {
	background:#ffc000;
	box-shadow:inset 5px 5px 10px 10px #ffd658;
}
.circle.yellow {
	background:#fffc00;
	box-shadow:inset 5px 5px 10px 10px #fdfdd4;
}
.circle.green {
	background:#acc956;
	box-shadow:inset 5px 5px 10px 10px #cadd94;
}
.circle span {
	font-size:67px;
	color:#fff;
	position:absolute;
	top:55%;
	left:50%;
	transform:translateX(-50%)translateY(-50%);
	-webkit-transform:translateX(-50%)translateY(-50%);
	-ms-transform:translateX(-50%)translateY(-50%);
	text-shadow:3px 3px 5px #999;
}
.dataLabel {
	width:236px;
	height:51px;
	background-size:contain !important;
	position:absolute;
	bottom:0;
	display:block;
}
.male .beratBadan .dataLabel {
	background:url(../images/img_male_beratbadan.png) no-repeat center bottom;
}
.male .tinggiBadan .dataLabel {
	background:url(../images/img_male_tinggibadan.png) no-repeat center bottom;
}
.female .beratBadan .dataLabel {
	background:url(../images/img_female_beratbadan.png) no-repeat center bottom;
}
.female .tinggiBadan .dataLabel {
	background:url(../images/img_female_tinggibadan.png) no-repeat center bottom;
}
.overallResult {
	max-width:360px;
	margin:0 auto 30px;
	text-align:center;
}
.overallResult p {
	font-size:13px;
	text-align:justify;
}
.resultBar {
	font-size:18px;
	color:#fff;
	padding:10px 25px;
	border-radius:15px;
	margin:0 auto 20px;
	display:inline-block;
	text-shadow:3px 3px 3px #333;
}
.red {
	background:#e00303;
}
.orange {
	background:#ffc000;
}
.yellow {
	background:#fffc00;
}
.green {
	background:#acc956;
}
.galleryMotto {
	text-align:center;
	margin:0 auto 40px;
}
.periodeTab {
	margin:0 auto 30px;
	text-align:center;
}
.periodeTab a {
	font-family: 'HelveticaNeueLTStd-Bd';
	font-size:18px;
	color:#6c3382;
	display:inline-block;
	padding:0 5px;
}
.periodeTab a:first-child {
	/* border-right:3px solid #6c3382; */
	padding-right:10px;
}
.periodeTab a.active {
	font-family: 'HelveticaNeueLTStd-Md';
}
.galleryWrapper {
	width:80%;
	max-width:880px;
	margin:0 auto 20px;
	display:block;
	text-align:center;
}
.galleryWrapper .previewBox {
	display:inline-block;
	margin:0 20px 40px;
}

/*----------------combodate+sod-----------------*/

.sod_select {
	width:18%;
	font-size:16px;
	color:#6c3382;
	border:3px solid #6c3382;
	border-radius:20px;
	padding:0 !important;
	margin-right:32px;
	text-align:center;
}
.sod_select.popupDropdown {
	margin-right:25px;
}
.sod_select:last-child {
	width:30%;
}
.sod_select::after {
	content:"";
}
.sod_select::before {
	content:"";
	width:26px;
	height:26px;
	background:url(../images/icon_dropdown.png) no-repeat center center;
	background-size:cover;
	display:block;
	top:5px;
	right:-34px;
}
.sod_select.popupDropdown::before {
	width:22px;
	height:22px;
	background:url(../images/icon_popupdropdown.png) no-repeat center center;
	background-size:cover;
	right:-30px;
}
.sod_select .sod_label {
	padding:10px;
}
.sod_select .sod_list_wrapper {
	width:100%;
	margin:0;
	border:inherit;
	border-radius:20px !important;
	overflow:hidden;
}
.sod_select:hover, .sod_select.open, .sod_select.focus {
	border-color:inherit;
}
.sod_select .sod_option.selected {
	padding:10px;
}
.sod_select .sod_option.selected::before {
	background:none;
}

@media only screen and (max-width:1080px) {
.headerWrapper {
	padding:0 20px;
}
.about .content {
	margin:0;
	padding:0 20px;
}
.about .content h4 {
	text-align:center;
}
.content {
	padding:0 20px;
}
.contentImg {
	max-width:300px;
}
}

@media only screen and (max-width:960px) {
nav ul li {
	margin:0 5px;
}
nav ul li a {
	font-size:14px;
}
.content p {
	font-size:16px !important;
}
.content ul li {
	font-size:16px;
}
}

@media only screen and (max-width:960px) {
.contentImg {
	max-width:250px;
}
}

@media only screen and (max-width:768px) {
.wrapper.about {
	min-height:auto;
	height:auto;
	background:#9dc2e0 url(../images/BG_elephantmobile.jpg) no-repeat bottom center;
	background-size:contain;
	padding-bottom:100%;
}
.burgerIcon,
.closeBtn {
	display:block;
}
nav {
	width:70%;
	min-height:100vh;
	background:#6c3382;
	position:fixed;
	top:0;
	right:-70%;
	padding:60px 0;
}
nav ul {
	padding:0;
}
nav ul li {
	display:block;
	text-align:left;
	margin:0;
}
nav ul li a {
	color:#fff;
	padding:10px 30px;
	display:block;
}
nav ul li a.active,
nav ul li a:hover {
	background:#fff;
	color:#6c3382;
	text-decoration:none;
}
.content ul {
	margin-right:0;
}
.content p {
	font-size:14px !important;
}
.content ul li {
	font-size:14px !important;
}
.content h3.tag {
	font-size:32px;
}
.content h3.tag span {
	font-size:20px;
}
.about .content h4 {
	font-size:22px;
}
.featureWrapper {
	width:340px;
}
.featurePanel {
	margin-bottom:40px;
}
.featurePanel:first-child {
	margin-right:0;
}
}

@media only screen and (max-width:680px) {
.resultWrapper,
.personalResult {
	width:100%;
}
.personalResult {
	height:auto;
	text-align:center;
	overflow:hidden;
}
.personalResult.male .umur {
	width:23% !important;
	margin-bottom:-100px !important;
