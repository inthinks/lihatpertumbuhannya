/* Reset */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td{margin:0;padding:0;}table{border-collapse:collapse;border-spacing:0;margin:0;padding:0;}fieldset,img{display:block; border:0;}address,caption,cite,code,dfn,em,strong,th,var{font-style:normal;font-weight:normal;}ol,ul {list-style:none;}caption,th {text-align:left;}h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}q:before,q:after{content:'';}abbr,acronym {border:0;}

body {
	font-family: 'HelveticaNeueLTStd-Md';
	font-size:16px;
}
b, strong {
	font-family: 'HelveticaNeueLTStd-Bd';
}
em {
	font-style:italic;
}
a {
	text-decoration:none;
}
* {
	-webkit-box-sizing:border-box;
	-moz-box-sizing:border-box;
	box-sizing:border-box;
}
.wrapper {
	width:100%;
	min-height:100vh;
	position:relative;
	display:block;
	background:url(../images/BG_purplegradient.png) repeat-x center bottom;
	background-attachment:fixed;
	overflow:hidden;
}
.wrapper.about {
	height:1750px;
	background:url(../images/BG_elephant.png) no-repeat top center;
	background-size:cover;
}
.headerWrapper {
	width:100%;
	max-width:1080px;
	margin:0 auto;
	position:relative;
}
header {
	text-align:center;
	padding:35px 0;
	float:left;
}
header .logo {
	width:177px;
	height:33px;
	background:url(../images/logo_pediasure.png) no-repeat center center;
	display:block;
	margin:0 auto;
}
nav {
	float:right;
	position:relative;
	z-index:1;
}
nav ul {
	text-align:center;
	padding:40px 0;
}
nav ul li {
	display:inline-block;
	margin:0 15px;
}
nav ul li a {
	font-family: 'GillSansMT';
	font-weight:600;
	color:#6c3382;
}
nav ul li a.active,
nav ul li a:hover {
	text-decoration:underline;
}
.mobilecloseBtn {
	width:28px;
	height:24px;
	background:url(../images/icon_close.png) no-repeat center center;
	display:none;
	text-indent:-9999px;
	position:absolute;
	top:30px;
	right:30px;
}
.burgerIcon {
	width:30px;
	height:25px;
	background:url(../images/icon_burger.png) no-repeat center center;
	text-indent:-9999px;
	display:none;
	position:absolute;
	top:30px;
	right:20px;
}
.content {
	width:100%;
	max-width:1080px;
	margin:0 auto;
	padding-bottom:100px;
}
.content h3.tag {
	width:100%;
	font-family: 'abbottsangregular';
	font-size:40px;
	color:#6d3483;
	text-align:center;
	display:inline-block;
	margin-bottom:35px;
}
.content h2 {
	font-size:24px;
	color:#6c3382;
}
.content p {
	color:#6c3382;
	line-height:1.4;
}
.contentImg {
	width:80%;
	max-width:414px;
	float:left;
}
.contentImg img {
	width:100%;
}
.content ul {
	margin-left:15px;
	float:left;
	margin-right:100px;
}
.content ul li {
	list-style:disc;
	padding-left:20px;
	line-height:1.5;
	color:#6c3382;
}
.about .content {
	max-width:830px;
	margin:0 0 0 60px;
}
.about .content h4 {
	font-family: 'HelveticaNeueLTStd-Bd';
	font-size:25px;
	color:#6c3382;
}
.about .content ul li {
	padding-left:0;
}
.productDesc {
	display:block;
	float:left;
	margin-top:20px;
	margin-bottom:50px;
}
.productDesc .image {
	width:25%;
	max-width:127px;
	float:left;
	margin-right:20px;
}
.productDesc .image img {
	width:100%;
}
.productDesc .caption {
	float:left;
	padding-top:30px;
}
.motto {
	width:100%;
	max-width:533px;
}
.motto img {
	width:100%;
	margin:0 auto;
}
footer {
	width:100%;
	position:absolute;
	bottom:0;
	left:0;
	display:block;
	overflow:hidden;
}
.footerWrapper {
	width:100%;
	height:300px;
	position:relative;
}
.footerWrapper .footerRibbon {
	width:100%;
	min-width:640px;
	display:block;
	position:absolute;
	bottom:0;
	right:0;
}
.footerWrapper .footercanCaption {
	min-width:70px;
	max-width:162px;
	width:11%;
	max-width:162px;
	display:block;
	position:absolute;
	bottom:0;
	right:6%;
}
.defBtn {
	font-size:18px;
	color:#fff;
	background:#6d3483;
	padding:15px 30px;
	border-radius:25px;
	display:inline-block;
	text-align:center;
	margin-bottom:10px;
	box-shadow:inset 6px 6px 10px 0 #aa88b7;
}
.defBtn.fbshare {
	width:100%;
	background:#3b5997;
	box-shadow:inset 4px 6px 5px 0 #8c9dc2;
	padding:10px;
}
.defBtn.twitshare {
	width:100%;
	background:#2eaae2;
	box-shadow:inset 4px 6px 5px 0 #82ccee;
	margin-bottom:0;
	padding:10px;
}
.defBtn.hover {
	box-shadow:inset 0 -6px 3px #aa88b7;
}
.featureWrapper {
	width:740px;
	margin:0 auto 50px;
	display:block;
}
.featurePanel {
	width:340px;
	float:left;
}
.featurePanel:first-child {
	margin-right:60px;
}
.featurePanel .image {
	width:100%;
	margin-bottom:20px;
}
.featurePanel .image img {
	width:100%;
}
.btnContainer {
	width:100%;
	position:relative;
	display:block;
	text-align:center;
	margin:0 auto 20px;
}
.featurePanel .btnContainer .defBtn {
	width:100%;
	margin:0 auto;
}
.featurePanel .clickArea {
	width:100%;
	height:100%;
	position:absolute;
	z-index:100;
}
.featurePanel p {
	width:100%;
	font-size:13px;
	color:#6c3382;
	text-align:center;
	display:inline-block;
	vertical-align:sub;
}
.popupWrapper {
	width:100%;
	height:100%;
	position:fixed;
	top:0;
	left:0;
	z-index:99999;
	display:none;
	overflow:auto;
}
.overlay {
	width:100%;
	height:100%;
	position:fixed;
	background:#000;
	opacity:0.8;
}
.popupBox {
	width:auto;
	position:absolute;
	top:50%;
	left:50%;
	transform:translateX(-50%)translateY(-50%);
	-webkit-transform:translateX(-50%)translateY(-50%);
	-ms-transform:translateX(-50%)translateY(-50%);
}
.popup {
}
#signupPopup {
	width:300px;
	background:#fff;
	border:5px solid #6d3483;
	border-radius:40px;
	padding:30px 20px;
	margin:0 auto;
}
.popup .closeBtn {
	font-size:24px;
	color:#fff;
	position:absolute;
	top:-15px;
	right:-15px;
}
.periode {
	text-align:center;
	margin:0 auto 50px;
}
.uploadBtn {
	width:340px;
	height:340px;
	position:relative;
}
.dropzone {
	padding:0;
	border:none;
	box-shadow:0 0 10px -5px #333;
	border-radius:20px;
}
.dropzoneBox {
	width:100%;
	height:100%;
}
#uploadFoto {
	width:100%;
	height:100%;
}
.dropzone.dz-clickable .dz-message, .dropzone.dz-clickable .dz-message * {
	background:url(../images/img_uploadbutton.png) no-repeat center center;
	width:100%;
	height:100%;
}
.dropzone .dz-message {
	width:100%;
	margin:0;
}
.dropzone .dz-preview {
	width:100%;
	height:100%;
    margin: 0 !important;
	top:0;
	left:0;
}
.dropzone .dz-preview .dz-image {
    border-radius:0 !important;
    height: 90% !important;
    width: 100% !important;
}
.dropzone .dz-preview .dz-image img {
    height: auto;
    margin: 0 auto;
    width: 100%;
}
.uploadData {
	width:340px;
	height:340px;
	background:#fff;
	box-shadow:0 0 10px -5px #333;
	border-radius:20px;
	position:relative;
}
.uploadData .input {
	width:85%;
	position:absolute;
	top:50%;
	left:50%;
	transform:translateX(-50%)translateY(-50%);
	-web-transform:translateX(-50%)translateY(-50%);
	-ms-transform:translateX(-50%)translateY(-50%);
}
.uploadData .input p,
.uploadData .input textarea {
	font-size:18px;
	color:#6c3381;
	text-align:left;
}
.uploadData .input textarea {
	width:100%;
	height:70px;
	line-height:35px;
	padding:0 10px;
	border:none;
	background:url(../images/img_underlinedot.png) no-repeat center bottom;
	background-size:contain;
	resize:none;
	margin-bottom:10px;
}

@media only screen and (max-width:1080px) {
.headerWrapper {
	padding:0 20px;
}
.about .content {
	margin:0;
	padding:0 20px;
}
.about .content h4 {
	text-align:center;
}
.content {
	padding:0 20px;
}
.contentImg {
	max-width:300px;
}
}

@media only screen and (max-width:960px) {
nav ul li {
	margin:0 5px;
}
nav ul li a {
	font-size:14px;
}
.content p {
	font-size:16px !important;
}
.content ul li {
	font-size:16px;
}
}

@media only screen and (max-width:960px) {
.contentImg {
	max-width:250px;
}
}

@media only screen and (max-width:768px) {
.wrapper.about {
	min-height:auto;
	height:auto;
	background:#9dc2e0 url(../images/BG_elephantmobile.jpg) no-repeat bottom center;
	background-size:contain;
	padding-bottom:100%;
}
.burgerIcon,
.closeBtn {
	display:block;
}
nav {
	width:70%;
	min-height:100vh;
	background:#6c3382;
	position:fixed;
	top:0;
	right:-70%;
	padding:60px 0;
}
nav ul {
	padding:0;
}
nav ul li {
	display:block;
	text-align:left;
	margin:0;
}
nav ul li a {
	color:#fff;
	padding:10px 30px;
	display:block;
}
nav ul li a.active,
nav ul li a:hover {
	background:#fff;
	color:#6c3382;
	text-decoration:none;
}
.content ul {
	margin-right:0;
}
.content p {
	font-size:14px !important;
}
.content ul li {
	font-size:14px !important;
}
.content h3.tag {
	font-size:32px;
}
.about .content h4 {
	font-size:22px;
}
.featureWrapper {
	width:340px;
}
.featurePanel {
	margin-bottom:40px;
}
.featurePanel:first-child {
	margin-right:0;
}
}

@media only screen and (max-width:560px) {
.content h3.tag {
	font-size:24px;
}
.content h2 {
	font-size:20px;
	margin-bottom:20px;
}
.about .content h4 {
	font-size:18px;
}
.footerWrapper .footerCan {
	bottom:10px;
}
}

@media only screen and (max-width:480px) {
.contentImg {
	float:none;
}
.featureWrapper {
	width:100%;
}
.featurePanel {
	width:100%;
	float:none;
	margin:0 auto 30px !important;
}
.uploadBtn {
	width:100%;
}
.uploadData {
	width:100%;
}
}